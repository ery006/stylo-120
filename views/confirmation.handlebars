<html>
	<head>
		<title>Stylo</title>
		<meta charset="utf-8">
		<meta name="viewport" content="width=device-width, initial-scale=1">
		<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.2.1/css/bootstrap.min.css">
		<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
		<link rel="stylesheet" href="css/home.css">
		  <link rel="stylesheet" href="//code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">

		  <link rel="stylesheet" href="https://www.w3schools.com/lib/w3-theme-blue-grey.css">
		<link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">

		<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
		<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.6/umd/popper.min.js"></script>
		<script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.2.1/js/bootstrap.min.js"></script>
		<script async defer type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCJdJ76C7tY7RfZpyaF3C5k4ysU_NkotzM"></script>
		<script src="https://developers.google.com/maps/documentation/javascript/examples/markerclusterer/markerclusterer.js">
	    </script>
	    <script src="https://cdn.rawgit.com/googlemaps/v3-utility-library/master/markerclustererplus/src/markerclusterer.js"></script>
		<script type="text/javascript" src="/js/map.js"></script>
		<script type="text/javascript" src="/js/addButton.js"></script>
		<script src="https://code.jquery.com/jquery-1.12.4.js"></script>
  <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
	</head>
	<body class="w3-theme-l5">
	<header class="w3-bar w3-card w3-theme">
			  <h1 class="myHeader">Add New Location</h1>
			</header>

		<div class="container text-center">
		<h3>New Location</h3>
			{{#each locations}}
			{{#if @last}}
					<div id="mySelect">
						
                			<p><h4>{{name}}</h4> </p>
							<div>{{description}}</div>
							<div>{{hours}}</div>
							<div>{{address}}</div>
							<div>{{seating}}</div>

    					</div>
					
					{{/if}}
	 			{{/each}}
		<br/>
		 <div>
			  <p>Thank you for submitting a new location! Your new addition has been added! You can now go back and see any of the maps through the tabs at the bottom (Home, Favorites, Study Groups).</p>
		</div>



		</div>
		<!-- Tabs -->
		<footer class="w3-row w3-theme" id="bottomBar">
			<!-- <div class="w3-row"> -->
			<div class="resizeButton">
			  <a href="/home" class="w3-button w3-hover-teal " role="button" id="bottomButton"><i class="fa fa-home"></i><br/>Home</a>
			  </div>
			  <div class="resizeButton">
			  <a href="/favoriteLocs" class="w3-button w3-hover-teal " role="button" id="bottomButton"><i class="fa fa-star"></i><br/>Favorite</a>
			  </div>
			  <div class="resizeButton2">
			  <a href="/studyGroups" class="w3-button w3-hover-teal " role="button" id="bottomButton"><i class="fa fa-users"></i><br/>Study Groups</a>
			  </div>
			  <!-- <button type="button" onclick="favoriteMap()" class="w3-button w3-hover-teal" id="bottomButton">Favorite</button> -->
<!-- 			  <button type="button" onclick="studyGroupsMap()" class="w3-button w3-hover-teal " id="bottomButton">Study Groups</button> -->
				<div class="resizeButton">
			  		<a href="/profile" class="w3-button w3-hover-teal " role="button" id="bottomButton"><i class="fa fa-user"></i><br/>Profile</a>
			   </div>
			<!-- </div> -->
		</footer>
	</body>
</html>
