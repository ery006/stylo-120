		<!-- Search bar -->
		<div class="w3-row w3-theme-l5">
			<div class="w3-container" id="searchbar">
				<form>
			  		<input type="text" placeholder="Search.." name="search" class="mySearchBar">
			  		<button type="button" onclick="mySearch()" class="w3-button w3-hover-teal"><i class="fa fa-search"></i></button>
			  		<div id="dialog" title="Filters">
					  	<p>
						  <label for="distance">Distance</label>
						  <input type="text" id="distance" readonly style="border:0; color:#f6931f; font-weight:bold;">
						</p>
						<div id="slider-range-max-dist" class="mySlider"></div>
						<p>
						  <label for="price">Price</label>
						  <input type="text" id="price" readonly style="border:0; color:#f6931f; font-weight:bold;">
						</p>
						<div id="slider-range-max-price" class="mySlider"></div>
						<fieldset>
						    <label>Sound </label><br/>
						    <label for="radio-1" class="myRadio">Quiet</label>
						    <input type="radio" name="radio-1" id="radio-1" class="myRadio">
						    <label for="radio-2" class="myRadio">Little Noise</label>
						    <input type="radio" name="radio-1" id="radio-2" class="myRadio">
						    <label for="radio-3" class="myRadio">Loud</label>
						    <input type="radio" name="radio-1" id="radio-3" class="myRadio">
						</fieldset>
						<input type="checkbox" name="filter" class="dropOptions" id="wifi"> Wifi<br>
						<input type="checkbox" name="filter" class="dropOptions" id="hour"> 24 Hour<br>
						<button type="button" onclick="filterMap()" class="btn btn-light" id="dropSubmit"> Submit </button>
					</div>
					
			  	</form>
				</div>

				<div class="w3-row" id="myRow">
				<label>Help</label>
				<div class="extraLabel"></div>
				<label >Add</label>
				<div class="extraLabel"></div>
				<label for="opener">Filter</label>
				<div class="extraLabel"></div>
				<label for="more">More</label>

			</div>

			<div class="w3-row" id="myRow">
					<div id="dialogColor" title="Color Meanings">
					  	<p>
							<font style="color:green">Green = Not Busy</font><br/>
							<font style="color:#e6e600">Yellow = Kind of Busy</font><br/>
							<font style="color:red">Red = Busy</font>
						</p>
				  	</div>
					<button id="openerColor" type="button" class="w3-button w3-hover-teal"> <i class="fa fa-question-circle"></i> </button>
					<div class="extra"></div>
				 	<a href="/add" id="addButton" class="w3-button w3-hover-teal" role="button"><i class="fa fa-plus"></i></a>
				 	<div class="extra"></div>
					<button type="button" class="w3-button w3-hover-teal" id="opener"><i class="fa fa-filter" name="opener"></i></button>
					<div class="extra"></div>
					<button type="button" class="w3-button w3-hover-teal" id="moreDeatils"><i class="fa fa-more" name="more"></i></button>
			</div>
