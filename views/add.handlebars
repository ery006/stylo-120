	<body>
		<div class="container text-center">
		<h1> Add New Locations </h1>

		<!-- Form -->
		<form>
			<div class="form-group">
			    <label for="exampleFormControlSelect1">Select One</label>
			    <select class="form-control" id="exampleFormControlSelect1">
			      <option>Add Study Location</option>
			      <option>Add Study Group</option>
			    </select>
			</div>
		  <div class="form-group">
		    <label for="exampleFormControlInput1">Location Name</label>
		    <input type="email" class="form-control" id="exampleFormControlInput1" placeholder="Name">
		  </div>
			<div class="form-group">
		    <label for="exampleFormControlInput2">Location Address</label>
		    <input type="email" class="form-control" id="exampleFormControlInput2" placeholder="123 Address St. La Jolla, CA">
		  </div>
		  <div class="form-group">
		    <label for="exampleFormControlTextarea1">Description</label>
		    <textarea class="form-control" id="exampleFormControlTextarea1" rows="3" placeholder="Description, Class/Topic, Pricing, etc..."></textarea>
		  </div>
		  <div id="dialog" title="Form was submitted">
			  <p>Thank you for submitting a new form! Your new addition has been added! You can now go back and see any of the maps through the tabs at the bottom (Home, Favorites, Study Groups).</p>
			</div>
				<button id="opener" type="button" class="btn btn-primary btn-lg">
					Submit
				</button>
		</form>

		<!-- Tabs -->
			<div class="container" style="text-align: center; position: fixed; bottom: 0">
			<div class="btn-group btn-group-lg">
			  <!-- <button type="button" onclick="/home" class="btn btn-light btn-lg">Home</button>
			  <button type="button" onclick="favoriteMap()" class="btn btn-light btn-lg">Favorite</button> -->
			  <a href="/home" class="btn btn-light btn-lg" role="button">Home</a>
			  <a href="/home" class="btn btn-light btn-lg" role="button">Favorite</a>
			  <a href="/home" class="btn btn-light btn-lg" role="button">Study Groups</a>
			  <!-- <button type="button" onclick="studyGroupsMap()" class="btn btn-light btn-lg">Study Groups</button> -->
			  <a href="/profile" class="btn btn-light btn-lg" role="button">Profile</a>
			  <!-- <button type="button" onclick="profilePage()" class="btn btn-light btn-lg">Profile</button> -->
			</div>
		</div>
	</div>
