		  <link rel="stylesheet" href="https://www.w3schools.com/lib/w3-theme-blue-grey.css">
		<link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">


		<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
		<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.6/umd/popper.min.js"></script>
		<script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.2.1/js/bootstrap.min.js"></script>
		<script async defer type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCJdJ76C7tY7RfZpyaF3C5k4ysU_NkotzM"></script>
		<script src="https://developers.google.com/maps/documentation/javascript/examples/markerclusterer/markerclusterer.js">
	    </script>
	    <script src="https://cdn.rawgit.com/googlemaps/v3-utility-library/master/markerclustererplus/src/markerclusterer.js"></script>
		<script type="text/javascript" src="/js/map.js"></script>
		<script type="text/javascript" src="/js/addButton.js"></script>
		<script src="https://code.jquery.com/jquery-1.12.4.js"></script>
  <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
	</head>
	<body class="w3-theme-l5">
	<header class="w3-bar w3-card w3-theme">
			  <h1 class="myHeader">Add New Location</h1>
			</header>
		<div class="container text-center">
<!-- 		<h2> Add New Locations </h2> -->

		<!-- Form -->
		<form>
			<div class="form-group">
			    <label for="exampleFormControlSelect1">Select One</label>
			    <select class="form-control" id="exampleFormControlSelect1">
			      <option>Add Study Location</option>
			      <option>Add Study Group</option>
			    </select>
			</div>
		  <div class="form-group">
		    <label for="exampleFormControlInput1">Location Name</label>
		    <input type="email" class="form-control" id="exampleFormControlInput1" placeholder="Name" name="name">
		  </div>
			<div class="form-group">
		    <label for="exampleFormControlInput2">Location Address</label>
		    <input type="email" class="form-control" id="exampleFormControlInput2" placeholder="123 Address St. La Jolla, CA" name="address">
		  </div>
		  <div class="form-group">
		    <label for="exampleFormControlTextarea1">Description</label>
		    <textarea class="form-control" id="exampleFormControlTextarea1" rows="3" placeholder="Description, Class/Topic, Pricing, etc..." name="description"></textarea>
		  </div>
		  <div id="dialog" title="Form was submitted">
			  <p>Thank you for submitting a new location! Your new addition has been added! You can now go back and see any of the maps through the tabs at the bottom (Home, Favorites, Study Groups).</p>
			</div>
				<button id="opener" type="button" class="w3-button w3-hover-teal ">
					Submit
				</button>
		</form>

		</div>
		<!-- Tabs -->
		<footer class="w3-row w3-theme" id="bottomBar">
			<!-- <div class="w3-row"> -->
			  <a href="/home" class="w3-button w3-hover-teal " role="button" id="bottomButton">Home</a>
			  <a href="/favoriteLocs" class="w3-button w3-hover-teal " role="button" id="bottomButton">Favorite</a>
			  <a href="/studyGroups" class="w3-button w3-hover-teal " role="button" id="bottomButton">Study Groups</a>
			  <!-- <button type="button" onclick="favoriteMap()" class="w3-button w3-hover-teal" id="bottomButton">Favorite</button> -->
<!-- 			  <button type="button" onclick="studyGroupsMap()" class="w3-button w3-hover-teal " id="bottomButton">Study Groups</button> -->
			  <a href="/profile" class="w3-button w3-hover-teal " role="button" id="bottomButton">Profile</a>
			<!-- </div> -->
		</footer>
	</body>
</html>
